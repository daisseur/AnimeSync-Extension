{
    "manifest_version": 3,
    "name": "Video Sync Extension",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://*.anime-sama.fr/*"],
        "js": ["contentScript.js"]
      }
    ]
  }
  